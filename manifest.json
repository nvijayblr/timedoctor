{
	"manifest_version": 2,
	"name": "Time Doctor Extension",
	"description": "Time Doctor Extension will anaylyze the user task details.",
	"version": "0.1",
	"icons": { "19": "img/icon_19.png" },
	"background": {
		"scripts": ["js/background.js"]
	},
	"browser_action": {
		"default_title": "Time Doctor",
		"default_icon": "img/icon_19.png",
		"default_popup": "popup.html"

	},
	"permissions": [
		"identity",
		"https://webapi.timedoctor.com/*"
	]
}
